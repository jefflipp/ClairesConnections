<section class="eventTable">
	<div class="container">
		<div class="row">
			<div class = "title text-center" >
				<h2> 
					Hi <%=current_user.user_name%> check out events from our network 
				</h2>
				<% @events.each do |e| %>
					<div class = "col-sm-3">
						<div class="list-group">
						  <a href="#" class="list-group-item active">
						    <%= e.title %>
						  </a>
						  <a href="#" class="list-group-item"><%= e.description %></a>
						  <a href="#" class="list-group-item"><%= e.date.strftime("%v") %></a>
						  <a href="#" class="list-group-item"><%= e.start_time.strftime("%r")  %></a>
						  <a href="#" class="list-group-item"><%= e.location %></a>
						   <button type="button" class="btn btn-alert"><%= link_to "More details", event_path(e) %> </button>
						</div>
					</div>
				<%end%>
			</div>	
		</div>
	</div>
</section>
	

		
		
		
