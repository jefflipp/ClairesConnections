
<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Claire's Connections</div>
  <div class="panel-body">
    <p>All Users</p>
  </div>

  <!-- Table -->
  <table class="table-condensed">

  <thead>
      <tr>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Username</th>
        <th>Email</th>
         <th>Action</th>
      </tr>
    </thead>
    <% @users.each do |user| %>
    <tbody>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.user_name %></td>
        <td><%= user.email %></td>
        <td><%= link_to "More", user_path(user) %>
    	<%= link_to "Edit", edit_user_path(user) %>
    	<%= link_to "Delete", user_path(user), method: :delete, data: { confirm: "Are you sure you want to delete this profile?"} %></td>
      </tr>
      <tr>
  
    <% end %>
  
  </table>
</div>