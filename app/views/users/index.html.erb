<h1>Users</h1>
<ul>
  <% @users.each do |user| %>
    <li><%= user.first_name %> <%= user.last_name %> <%= user.user_name %> <%= link_to "More", user_path(user) %>
    	<%= link_to "Edit", edit_user_path(user) %>
    	<%= link_to "Delete", user_path(user), method: :delete, data: { confirm: "Are you sure you want to delete this profile?"} %>
</li>
  <% end %>
</ul>
