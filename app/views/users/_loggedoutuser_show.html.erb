<div class = "container">
  <div class = "title">
    <h1> This is the profile for <%= @user.user_name %> </h1>
  <section class="stats">
      <%= render 'sessions/stats' %>
    </section>
  <div class="row text-center">
    <div class="col-sm-3">
      <div class="thumbnail">
        <img src="https://cdn4.iconfinder.com/data/icons/ionicons/512/icon-person-128.png" alt="...">
        <div class="caption text-center">
          <ul>
            <li><strong> Name: </strong> <%=@user.first_name%></li>
            <li><strong> Email: </strong> <%=@user.email%></li>
            <li><strong> About me: </strong> <%=@user.bio%><li/>
          <br>
          </ul>
        </div>
      </div>
    </div>
  </div>
          